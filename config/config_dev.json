{
	"model": "DeepLabV3Plus",
	"enc": "timm-regnety_320",
	"enc_weights": "imagenet",
	"epochs" : 100,
	
	"loss": "CE",
	"loss_weight": 0.5,

	"optimizer": "Adam",
	"weight_decay" : 1e-6,

	"batch_size" : 8,
	"learning_rate" : 1e-4,
	"lr_scheduler": "SGDR",
	"lr_min": 1e-6,
	"lr_max": 1e-4,
	"lr_max_decay": 0.8,
	"T": 4,
	"T_warmup": 2,
	"T_mult": 2,

	"aug": "no"
}
